{
  "manifest_version": 3,
  "name": "WICKY Extension",
  "version": "1.0",
  "description": "A scaffold for a Chrome extension.",
  "action": {
    "default_title": "Open WICKY Side Panel"
  },
  "side_panel": {
    "default_path": "sidepanel.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage", "sidePanel", "activeTab", "scripting"],
  "host_permissions": ["<all_urls>"],
  "web_accessible_resources": [{
    "resources": ["data-extractor.js"],
    "matches": ["<all_urls>"]
  }]
}
